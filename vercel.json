{
  "rewrites": [
    { 
      "source": "/api/:path*", 
      "destination": "https://spotlybackend.onrender.com/api/:path*" 
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { 
          "key": "Access-Control-Allow-Origin", 
          "value": "*" 
        }
      ]
    }
  ]
}